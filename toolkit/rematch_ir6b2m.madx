     constraint,weight=1000,expr= abs(refdmuxkickb2_tcdq-0.250)<0.0111;
  };

  !if(match_on_dump>0 && bxdumpb2>0 && bydumpb2>0){
  if(match_on_dump>0)
   {
     constraint,expr=refbxdumpb2=bxdumpb2;
     constraint,expr=refbydumpb2=bydumpb2;
   };
