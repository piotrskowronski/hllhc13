  !constraint,sequence=lhcb2,range=ip6,alfx=alfxip6b2;
  !constraint,sequence=lhcb2,range=ip6,alfy=alfyip6b2;
  !constraint,sequence=lhcb2,range=ip6,dx=dxip6b2;
  !constraint,sequence=lhcb2,range=ip6,dpx=dpxip6b2;
