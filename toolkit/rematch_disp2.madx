};

GENER_BUMP(ex1,ex2) : macro = {

!Generate the bumps to correct Dx/Dy
kmcb_max=80.e-6;

 on_x1aux=on_x1;on_sep1aux=on_sep1;
 on_x2aux=on_x2;on_sep2aux=on_sep2;on_aliceaux=on_alice;
 on_x5aux=on_x5;on_sep5aux=on_sep5;
